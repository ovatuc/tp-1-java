//7.- Escribe un programa que pida 3 nÃºmeros y escriba en la pantalla el mayor de los tres.

let numero1, numero2, numero3, t, mayor;
numero1 = parseInt(prompt('ingrese el primer numero'));
numero2 = parseInt(prompt('ingrese el segundo numero'));
numero3 = parseInt(prompt('ingrese el tercer numero'));t

if(numero1 > numero2){
    t = numero1;
}else{
    t = numero2;
}if(t > numero3){
    mayor = t;
}else{
    mayor = numero3;
}
document.write('El numero mayor es: ' +mayor);



